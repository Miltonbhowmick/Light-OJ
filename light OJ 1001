#include<iostream>
#include<string>
#include<cstring>
#include<cstdio>

using namespace std;

int main()
{
    int t,n;
    cin>>t;
    while(t--){
        cin>>n;
        if(n>10){
            cout<<n/2<<" "<<n-(n/2)<<endl;
        }
        else
            cout<<0<<" "<<n<<endl;
    }
    return 0;
}
